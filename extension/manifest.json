{
    "manifest_version": 3,
    "name": "Sponsoshield",
    "version": "1.0",
    "description": "Sponso-blocker",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ]
  }